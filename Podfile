# platform :ios, '9.0'

target 'LiveArena' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

	pod 'Firebase/Analytics'
	pod 'Firebase/Auth'
	pod ‘Firebase/Core‘
	pod ‘Firebase/Database‘
	pod 'lottie-ios'
	
	

  # Pods for LiveArena

  target 'LiveArenaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LiveArenaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
